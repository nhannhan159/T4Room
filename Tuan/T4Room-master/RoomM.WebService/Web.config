      </service>
      
        <service name="RoomM.WebService.StaffService" behaviorConfiguration="ServiceMEXBehavior">
          <endpoint address="StaffService" binding="basicHttpBinding" contract="RoomM.WebService.IStaffService"/>
          <endpoint address="" binding="netTcpBinding" contract="RoomM.WebService.IStaffService"/>
          <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
          <host>
