          </security>
        </binding>
      </basicHttpBinding>
    </bindings>-->
    <services>
      <service name="RoomM.WebService.RoomService"
               behaviorConfiguration= "ServiceMEXBehavior">

        <!--<endpoint address="RoomService"
                  binding="basicHttpBinding"
                  contract="RoomM.WebService.IRoomService"
                  >
          
        </endpoint>-->

        <endpoint address=""
                  binding="netTcpBinding"
                  contract="RoomM.WebService.IRoomService">
          <identity>
